<?xml version="1.0"?>
<rules>
	<rule name="Lot">
		<operator name="translate">
			<param name="mode" value="rel"/>
			<param name="coordSystem" value="world"/>
			<param name="x" value="0"/>
			<param name="y" value="0"/>
			<param name="z" value="0.3"/>
		</operator>
		<operator name="offset">
			<param name="offsetDistance" value="-1"/>
		</operator>
		<operator name="comp">
			<param name="inside" value="X"/>
			<param name="border" value="NIL"/>
		</operator>
	</rule>
	<rule name="X">
		<operator name="color">
			<param name="r" value="1"/>
			<param name="g" value="0"/>
			<param name="b" value="0"/>
		</operator>
	</rule>
</rules>
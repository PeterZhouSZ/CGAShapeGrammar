<?xml version="1.0"?>
<rules>
	<rule name="Lot">
		<translate mode="rel" coordSystem="world">
			<param name="x" type="absolute" value="0"/>
			<param name="y" type="absolute" value="0"/>
			<param name="z" type="absolute" value="0.3"/>
		</translate>
		<offset offsetDistance="-1"/>
		<comp>
			<param name="inside" value="X"/>
			<param name="border" value="NIL"/>
		</comp>
	</rule>
	<rule name="X">
		<color r="1" g="0" b="0"/>
	</rule>
</rules>
<?xml version="1.0"?>
<rules>
	<rule name="Lot">
		<translate>
			<param name="mode" value="rel"/>
			<param name="coordSystem" value="world"/>
			<param name="x" value="0"/>
			<param name="y" value="0"/>
			<param name="z" value="0.3"/>
		</translate>
		<!--
		<operator name="split">
			<param name="splitAxis" value="y"/>
			<param name="size">
				<element type="absolute" value="1.0"/>
				<element type="floating" value="1.5" repeat="true"/>
			</param>
			<param name="name">
				<element value="Parcel."/>
				<element value="Parcel."/>
			</param>
		</operator>
		-->
		<split>
			<param name="splitAxis" value="y"/>
			<param name="size">
				<element type="absolute" value="2.0"/>
				<element type="floating" value="2.5" repeat="true"/>
				<element type="floating" value="2.5" />
				<element type="absolute" value="2.0"/>
			</param>
			<param name="name">
				<element value="Wall"/>
				<element value="Tile"/>
				<element value="EntranceTile"/>
				<element value="Wall"/>
			</param>
		</split>
	</rule>
</rules>
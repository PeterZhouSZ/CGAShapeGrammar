<?xml version="1.0"?>
<rules>
	<rule name="Lot">
		<operator name="translate">
			<param name="mode" value="rel"/>
			<param name="coordSystem" value="world"/>
			<param name="x" value="0"/>
			<param name="y" value="0"/>
			<param name="z" value="0.3"/>
		</operator>
		<operator name="split">
			<param name="splitAxis" value="y"/>
			<param name="size">
				<element type="absolute" value="1.0"/>
				<element type="floating" value="1.5" repeat="true"/>
			</param>
			<param name="name">
				<element value="Parcel"/>
				<element value="Parcel"/>
			</param>
		</operator>
	</rule>
</rules>
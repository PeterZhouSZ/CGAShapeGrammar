<?xml version="1.0"?>
<rules>
	<rule name="Lot">
		<operator name="extrude">
			<param name="height" value="5"/>
		</operator>
		<operator name="split">
			<param name="splitAxis" value="z"/>
			<param name="size">
				<element type="floating" value="1.5" repeat="true"/>
			</param>
			<param name="name">
				<element value="X"/>
			</param>
		</operator>
	</rule>

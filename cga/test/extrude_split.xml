<?xml version="1.0"?>
<rules>
	<rule name="Lot">
		<extrude>
			<param name="height" value="5"/>
		</extrude>
		<split>
			<param name="splitAxis" value="y"/>
			<param name="size">
				<element type="absolute" value="2.0"/>
				<element type="floating" value="1.5" repeat="true"/>
			</param>
			<param name="name">
				<element value="Floor."/>
				<element value="Floor."/>
			</param>
		</split>
	</rule>
</rules>
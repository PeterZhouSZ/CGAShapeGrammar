<?xml version="1.0"?>
<rules>
	<rule input_name="Lot" op="taper">
		<param name="height" value="11"/>
		<param name="top_ratio" value="0.7"/>
		<param name="output_name" value="Body"/>
	</rule>
	<rule input_name="Body" op="comp">
		<param name="front" value="Front"/>
		<param name="side" value="Side"/>
		<param name="top" value="Top"/>
		<param name="bottom" value="Bottom"/>
	</rule>
	<rule input_name="Top" op="offset">
		<param name="offsetDistance" value="0.2"/>
		<param name="output_name" value="CapBase"/>
	</rule>
	<rule input_name="CapBase" op="extrude">
		<param name="height" value="6"/>
		<param name="output_name" value="Cap"/>
	</rule>
</rules>
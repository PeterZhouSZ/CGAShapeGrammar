<?xml version="1.0"?>
<rules>
	<rule name="Lot">
		<operator name="taper">
			<param name="height" value="11"/>
			<param name="top_ratio" value="0.7"/>
		</operator>
		<output name="Body"/>
	</rule>
	<rule name="Body">
		<operator name="comp">
			<param name="front" value="Front"/>
			<param name="side" value="Side"/>
			<param name="top" value="Top"/>
			<param name="bottom" value="Bottom"/>
		</operator>
	</rule>
	<rule name="Top">
		<operator name="offset">
			<param name="offsetDistance" value="0.2"/>
		</operator>
		<operator name="extrude">
			<param name="height" value="6"/>
		</operator>
	</rule>
</rules>

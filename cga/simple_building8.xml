<?xml version="1.0"?>
<rules>
	<rule input_name="Lot" op="extrude">
		<param name="height" value="11"/>
		<param name="output_name" value="Building"/>
	</rule>
	<rule input_name="Building" op="comp">
		<param name="front" value="FrontFacade"/>
		<param name="side" value="SideFacade"/>
		<param name="top" value="Roof"/>
		<param name="bottom" value="Base"/>
	</rule>
	<rule input_name="FrontFacade" op="setupProjection">
		<param name="coordinateType" value="absolute"/>
		<param name="texWidth" value="2.5"/>
		<param name="texHeight" value="1"/>
		<param name="output_name" value="FrontFacade_1"/>
	</rule>
	<rule input_name="FrontFacade_1" op="texture">
		<param name="texture" value="textures/brick.jpg"/>
		<param name="output_name" value="FrontFacade_2"/>
	</rule>
	<rule input_name="SideFacade" op="setupProjection">
		<param name="coordinateType" value="absolute"/>
		<param name="texWidth" value="2.5"/>
		<param name="texHeight" value="1"/>
		<param name="output_name" value="SideFacade_1"/>
	</rule>
	<rule input_name="SideFacade_1" op="texture">
		<param name="texture" value="textures/brick.jpg"/>
		<param name="output_name" value="SideFacade_2"/>
	</rule>
	<rule input_name="FrontFacade_2" op="split">
		<param name="direction" value="y"/>
		<param name="size">
			<element type="absolute" value="4.0"/>
			<element type="floating" value="3.5" repeat="true"/>
		</param>
		<param name="name">
			<element value="Floor"/>
			<element value="Floor"/>
		</param>
	</rule>
	<rule input_name="SideFacade_2" op="split">
		<param name="direction" value="y"/>
		<param name="size">
			<element type="absolute" value="4.0"/>
			<element type="floating" value="3.5" repeat="true"/>
		</param>
		<param name="name">
			<element value="Floor"/>
			<element value="Floor"/>
		</param>
	</rule>
	<rule input_name="Floor" op="split">
		<param name="direction" value="x"/>
		<param name="size">
			<element type="absolute" value="1.0"/>
			<element type="floating" value="3.0" repeat="true"/>
			<element type="absolute" value="1.0"/>
		</param>
		<param name="name">
			<element value="Wall"/>
			<element value="Tile"/>
			<element value="Wall"/>
		</param>
	</rule>
	<rule input_name="Roof" op="setupProjection">
		<param name="coordinateType" value="relative"/>
		<param name="texWidth" value="0"/>
		<param name="texHeight" value="0"/>
		<param name="output_name" value="Roof_1"/>
	</rule>
	<rule input_name="Roof_1" op="texture">
		<param name="texture" value="textures/roof.jpg"/>
		<param name="output_name" value="Roof_2"/>
	</rule>
	<rule input_name="Tile" op="split">
		<param name="direction" value="x"/>
		<param name="size">
			<element type="floating" value="1.0"/>
			<element type="absolute" value="2.0"/>
			<element type="floating" value="1.0"/>
		</param>
		<param name="name">
			<element value="Wall"/>
			<element value="WallWindowWall"/>
			<element value="Wall"/>
		</param>
	</rule>
	<rule input_name="WallWindowWall" op="split">
		<param name="direction" value="y"/>
		<param name="size">
			<element type="absolute" value="1.0"/>
			<element type="absolute" value="1.5"/>
			<element type="floating" value="1.0"/>
		</param>
		<param name="name">
			<element value="Wall"/>
			<element value="Window"/>
			<element value="Wall"/>
		</param>
	</rule>
	<rule input_name="Window" op="translate">
		<param name="x" value="0"/>
		<param name="y" value="0"/>
		<param name="z" value="-0.25"/>
		<param name="output_name" value="Window_1"/>
	</rule>
	<rule input_name="Window_1" op="setupProjection">
		<param name="coordinateType" value="relative"/>
		<param name="texWidth" value="0"/>
		<param name="texHeight" value="0"/>
		<param name="output_name" value="Window_2"/>
	</rule>
	<rule input_name="Window_2" op="texture">
		<param name="texture" value="textures/window.jpg"/>
		<param name="output_name" value="Window_3"/>
	</rule>
</rules>